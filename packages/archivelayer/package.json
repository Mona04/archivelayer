    "name": "Mona04",
    "email": "moksha1905@gmail.com"
  },
    
  "type": "module",
  "main": "dist/index.js",
  "bin": "./bin/cli.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index-cjs.cjs"
    },
    "./hooks": {
      "import": "./dist/hooks/index.js",
      "require": "./dist/hooks/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "packge.json": [
        "./package.json"
      ],
      ".": [
        "./dist"
      ],
      "hooks": [
        "./dist/hooks"
      ]
    }
  },

  "workspaces": [
    "./*"
  ],
  "dependencies": {
    "@archivelayer/utils": "file:../@archivelayer/utils",
    "@archivelayer/core": "file:../@archivelayer/core",

    "commander": "12.0.0",
    "typescript": "5.3.3",
    "react": "18.2.0",

    "fs": "^0.0.1-security",    
    "@mdx-js/mdx": "3.0.1"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/node": "20.10.5",
    "@types/react": "18.2.45"
  }
}